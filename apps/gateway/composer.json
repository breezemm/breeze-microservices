{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "backpack/logmanager": "^5.0",
        "bensampo/laravel-enum": "^6.3",
        "glorand/laravel-model-settings": "^6.0",
        "guzzlehttp/guzzle": "*",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.21",
        "laravel/octane": "^2.3",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mateusjunges/laravel-kafka": "^1.13",
        "opcodesio/log-viewer": "^3.1",
        "overtrue/laravel-follow": "^5.1",
        "overtrue/laravel-like": "^5.1",
        "predis/predis": "^2.2",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.10",
        "staudenmeir/laravel-adjacency-list": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-stressless": "^2.2",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -rCte \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "setup": [
            "php artisan migrate:fresh --seed",
            "php artisan passport:install",
            "php artisan storage:link"
        ],
        "test": "pest",
        "format": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
