{
    "name": "myanmarcyberyouths/breeze-core-sdk",
    "description": "This is my package breeze-core-sdk",
    "keywords": [
        "myanmarcyberyouths",
        "laravel",
        "breeze-core-sdk"
    ],
    "homepage": "https://github.com/myanmarcyberyouths/breeze-core-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Aung Myat Moe",
            "email": "aungmyatmoe834@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^10.0||^11.0",
        "saloonphp/saloon": "^3.0",
        "spatie/laravel-data": "^4.6",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "MyanmarCyberYouths\\Breeze\\": "src/",
            "MyanmarCyberYouths\\Breeze\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyanmarCyberYouths\\Breeze\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-breeze-core-sdk --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyanmarCyberYouths\\Breeze\\BreezeServiceProvider"
            ],
            "aliases": {
                "Breeze": "MyanmarCyberYouths\\Breeze\\Facades\\Breeze"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
