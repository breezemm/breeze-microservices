[mysqld]
skip-federated
skip-name-resolve

host_cache_size   = 0

bind-address        = 0.0.0.0
mysqlx-bind-address	= 0.0.0.0

default-time-zone = 'Asia/Yangon'

datadir           = /var/lib/mysql
socket            = /var/lib/mysql/mysql.sock
secure-file-priv  = /var/lib/mysql-files

pid-file          = /var/run/mysqld/mysqld.pid

user              = mysql
